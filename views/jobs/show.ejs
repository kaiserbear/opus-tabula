<% include ../partials/header %>
<div class="container">
    <div class="row">
        <div class="col-md-3">
            <p class="lead">Job Camp</p>
           
        </div>
        <div class="col-md-9">
            <div class="thumbnail">
                <img class="img-responsive" src="<%= job.image %>">
                <div class="caption-full">
                    <h4><a><%=job.name%></a></h4>
                    <p><%= job.description %></p>
                    <p>
                        <em>Submitted By <%= job.author.username %></em>
                    </p>
                </div>
            </div>
        </div>
    </div>
</div>

<% include ../partials/footer %>